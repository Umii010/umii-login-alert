{
    "name": "umii/login-alert",
    "description": "Send login alerts via email/SMS when users sign in. Includes IP, device info, and optional new-device detection.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Muhammad Umer Shahzad",
            "email": "umii020@hotmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/notifications": "^10.0|^11.0|^12.0",
        "illuminate/events": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/auth": "^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Umii\\LoginAlert\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Umii\\LoginAlert\\LoginAlertServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
